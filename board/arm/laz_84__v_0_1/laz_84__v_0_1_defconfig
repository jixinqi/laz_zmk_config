# SoC
CONFIG_SOC_SERIES_STM32F1X=y
CONFIG_SOC_STM32F103X8=y

# 72MHz system clock
CONFIG_SYS_CLOCK_HW_CYCLES_PER_SEC=72000000

# enable pinmux
CONFIG_PINMUX=y
CONFIG_PINCTRL=y

# enable GPIO
CONFIG_GPIO=y

# enable clocks
CONFIG_CLOCK_CONTROL=y

# USB
CONFIG_USB_DEVICE_MANUFACTURER="Laz"
CONFIG_USB_DEVICE_PRODUCT="Laz84_Keyboard"
